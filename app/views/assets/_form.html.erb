<%= form_for @asset, :html => {:multipart => true} do |f| %>
  <%= f.error_messages %>
  

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <p>
    <%= f.file_field :uploaded_file%><br />
  </p>
  <p>
    <%= f.hidden_field :folder_id %>
  </p>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
